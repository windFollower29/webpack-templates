{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "targets": {
          "ie": "9",
          "chrome": "58"
        }
      }
    ],
    // '@babel/preset-react'
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose" : true }],
    "@babel/plugin-syntax-dynamic-import",
    // "@loadable/babel-plugin",
    // [
    //   "@babel/transform-runtime",
    //   {
    //     "regenerator": true,
    //     // "corejs": 2
    //   }
    // ],
    "@babel/transform-regenerator"
  ]
}